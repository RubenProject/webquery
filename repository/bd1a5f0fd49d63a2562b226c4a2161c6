            <strong>6.500</strong>
            <span>employees</span>
          </li>
        <li class="diversity">
            <strong>23,5%</strong>
            <span>female professors</span>
          </li>
        <li class="nobelprice">
            <strong>15</strong>
            <span>Nobel Prizes</span>
          </li>
        <li class="firstyears">
            <strong>26.853</strong>
            <span>students</span>
          </li>
        </ul>
